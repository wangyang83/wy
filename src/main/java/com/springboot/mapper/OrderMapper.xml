<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springboot.mapper.OrderMapper">

    <select id="selectOrder" parameterType="com.springboot.domain.search.OrderSearch" resultType="com.springboot.domain.entity.Order">
        SELECT * FROM `order` where orderNo = #{orderNo}
    </select>

</mapper>